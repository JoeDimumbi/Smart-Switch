                      00002 ;***********************************************SMART SWITCH********************************************
                            *******************
                      00003 ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;FILOSOFIA DE OPERACION:;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                            ;;;;;;;;;;;;;;;;;
                      00004 ;EL SENSOR PIR (GPIO 0) DETECTARA PRESENCIA EN EL CUARTO,  SI LA HAY, EL MICROCONTROLADOR VERIFICARA LA 
                            CANTIDAD DE LUZ EN
                      00005 ;EL CUARTO, CENSADA POR LA LDR (GPIO 1) EN CASO QUE EXISTA POCA LUZ MANDARA UN NIVEL ALTO Y ENCENDERÁ LA
                             LÁMPARA (GPIO 4)
                      00006 ;LA CUAL ESTARÁ CONTROLADA POR UN RELEVADOR. EN CASO DE QUE EXISTA SUFICIENTE ILUMINACIÓN EN LA HABITACI
                            ÓN NO ENCENDERÁ
                      00007 ;LA LÁMPARA Y, VOLVERÁ A VERIFICAR LOS SENSORES DE PRESENCIA (PIR) Y DE LUZ (LDR).
                      00008 ;SI SE DESEA MANIPULAR LA CARGA SIN IMPORTAR EL ESTADO DE LOS SENSORES; EXISTIRÁ SWITCH ÓPTICO (GPIO 3) 
                            SIENDO
                      00009 ;UN EMISOR Y RECEPTOR INFRARROJO; EL CUAL AL PASAR LA MANO POR ÉL DEBERÁ MANDAR NIVEL ALTO, CHECARA SI L
                            A LÁMPARA
                      00010 ;ESTÁ ENCENDIDA O APAGADA, EN CASO DE QUE  ESTE LA APAGARA LA ENCENDERÁ Y EN CASO CONTRARIO LA APAGARA; 
                            EN CASO
                      00011 ;DE HABERLA ACTIVADO LA CARGA ESPERARA 5 MIN PARA DESACTIVARLA, CUANDO TERMINE EL TIEMPO ESTABLECIDO VER
                            IFICARA  LOS
                      00012 ;SENSORES Y ACTUARA EN FUNCIÓN DE LO CENSADO.
                      00013 ;
                      00014 ;EL CICLADO PRINCIPAL SERÁ VERIFICANDO EL ESTADO DEL SENSOR PIR Y EL SWITCH ÓPTICO. HASTA QUE ALGUNO DE 
                            LOS
                      00015 ;DOS CAMBIE DE ESTADO ACTIVARA LA CARGA.
                      00016 ;
                      00017 ;EL SENSOR DE LUZ (LDR) DEBE APUNTAR A LUGAR OPUESTO AL FOCO, ENFOCAR DE PREFERENCIA A LA VENTANA. PARA 
                            EVITAR
                      00018 ;SWITCHEOS EN FALSO. CONTARÁ CON UN POTENCIÓMETRO PARA AJUSTAR LA SENSIBILIDAD DE LA LUZ.
                      00019 ;
                      00020 ;MIENTRAS ESTE ACTIVADA LA CARGA, SE PUEDE DESACTIVAR EN EL MOMENTO QUE UNO LO DESEE PERO EN CASO QUE EX
                            ISTA
                      00021 ;PRESENCIA NO PODRÁ DESACTIVARLA HASTA QUE CAMBIE ÉSTE ESTADO.
                      00022 ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
